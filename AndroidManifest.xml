<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.nau.granroth_m_mitchell_j_anagramapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AboutPage" />
        <activity android:name=".GameplayPage" />
        <activity android:name=".ChallengeSelectionPage" />
        <activity android:name=".LevelSelectionPage" />
        <activity android:name=".ResultsPage" />
        <activity android:name=".PostGameOptionsPage" />
        <activity android:name=".DetailedResultsPage" />
    </application>

</manifest>